<h2>CreateUser</h2>
@using (Html.BeginForm("CreateUser", "Admin", FormMethod.Post))
{
    <div class="row">
        <div class="col-md-10">
            <label class="control-label">User Email</label>
            <input type="text" name="txtEmail" class="form-control" />
        </div>
        <div class="col-md-6">
            <label class="control-label">Passowrd</label>
            <input type="text" name="txtPassword" class="form-control" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <button type="submit" value="Submit" class="btn btn-primary">Save</button>
        </div>
    </div>
    <br />
    <div style="text-decoration:none" class="btn btn-success btn-sm">
        @Html.ActionLink("Back to List", "Index")
    </div>
}
